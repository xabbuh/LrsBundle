{
    "name": "xabbuh/lrs-bundle",
    "description": "A Symfony bundle providing an xAPI Learning Record Store",
    "type": "symfony-bundle",
    "keywords": ["xAPI", "Experience API", "Tin Can API", "LRS", "Learning Record Store", "Symfony", "bundle"],
    "homepage": "https://github.com/xabbuh/LrsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Flothmann",
            "homepage": "https://github.com/xabbuh"
        }
    ],
    "require": {
        "jms/serializer-bundle": "~0.9",
        "php-xapi/exception": "^0.1.0",
        "php-xapi/model": "^0.1.0",
        "php-xapi/serializer": "^0.1.0",
        "php-xapi/repository-implementation": "^0.1.0",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/http-foundation": "~2.3",
        "symfony/http-kernel": "~2.3"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "~0.6",
        "phpunit/phpunit": "~4.0",
        "php-xapi/repository-mongodb": "^0.1.0",
        "php-xapi/test-fixtures": "^0.1.0"
    },
    "suggest": {
        "php-xapi/repository-mongodb": "Allows to store statements in MongoDB"
    },
    "autoload": {
        "psr-4": {
            "Xabbuh\\XApi\\Bundle\\LrsBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
